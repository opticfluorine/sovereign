    <StartupObject>Sovereign.StandaloneWorldGen.Main.StandaloneWorldGenMain</StartupObject>
    <RootNamespace>Sovereign.StandaloneWorldGen</RootNamespace>
    <AssemblyName>Sovereign.StandaloneWorldGen</AssemblyName>
